<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/navigation"
    app:startDestination="@id/splashFragment">
    <fragment
        android:id="@+id/mainFragment"
        android:name="com.example.userapp.ui.main.MainFragment"
        android:label="MainFragment" >
        <action
            app:destination="@+id/communityPreviewFragment"
            android:id="@+id/action_mainFragment_to_communityPreviewFragment"/>
        <action
            app:destination="@+id/reservationFacilitySelectFragment"
            android:id="@+id/action_mainFragment_to_reservationFacility">
            <argument
                android:name="myArg"
                app:argType="com.example.userapp.data.model.ReservationFacility" />
        </action>
        <action
            android:id="@+id/action_mainFragment_to_mainhomeNoticeFragment"
            app:destination="@id/mainhomeNoticeFragment" />

        <action
            android:id="@+id/action_mainFragment_to_settingsChangeInfoFragment"
            app:destination="@id/settingsChangeInfoFragment"
            app:enterAnim="@anim/slide_enter"
            app:exitAnim="@anim/slide_exit"
            app:popEnterAnim="@anim/slide_pop_enter"
            app:popExitAnim="@anim/slide_pop_exit"/>
        <action
            android:id="@+id/action_mainFragment_to_settingsChangePwdFragment"
            app:destination="@id/settingsChangePwdFragment"
            app:enterAnim="@anim/slide_enter"
            app:exitAnim="@anim/slide_exit"
            app:popEnterAnim="@anim/slide_pop_enter"
            app:popExitAnim="@anim/slide_pop_exit"/>
    </fragment>
    <fragment
        android:id="@+id/mainhomeNoticeFragment"
        android:name="com.example.userapp.ui.main.home.HomeNoticeFragment"/>
    <fragment
        android:id="@+id/splashFragment"
        android:name="com.example.userapp.ui.splash.SplashFragment"
        android:label="SplashFragment" >
        <action
            android:id="@+id/action_splashFragment_to_mainFragment"
            app:destination="@id/mainFragment"
            app:enterAnim="@anim/slide_enter"
            app:exitAnim="@anim/slide_exit"
            app:popEnterAnim="@anim/slide_pop_enter"
            app:popExitAnim="@anim/slide_pop_exit"
            app:popUpTo="@+id/splashFragment"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_splashFragment_to_introFragment"
            app:destination="@id/introFragment"
            app:enterAnim="@anim/slide_enter"
            app:exitAnim="@anim/slide_exit"
            app:popEnterAnim="@anim/slide_pop_enter"
            app:popExitAnim="@anim/slide_pop_exit"
            app:popUpTo="@+id/splashFragment"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/introFragment"
        android:name="com.example.userapp.ui.intro.IntroFragment"
        android:label="IntroFragment" >
        <action
            android:id="@+id/action_introFragment_to_signInGraph"
            app:destination="@id/signInGraph"
            app:enterAnim="@anim/slide_enter"
            app:exitAnim="@anim/slide_exit"
            app:popEnterAnim="@anim/slide_pop_enter"
            app:popExitAnim="@anim/slide_pop_exit"/>
        <action
            android:id="@+id/action_introFragment_to_signUpGraph"
            app:destination="@id/signUpGraph"
            app:enterAnim="@anim/slide_enter"
            app:exitAnim="@anim/slide_exit"
            app:popEnterAnim="@anim/slide_pop_enter"
            app:popExitAnim="@anim/slide_pop_exit"/>
    </fragment>
    <action
        android:id="@+id/action_global_introFragment"
        app:destination="@id/introFragment"
        app:enterAnim="@anim/nav_default_enter_anim"
        app:exitAnim="@anim/nav_default_exit_anim"
        app:popEnterAnim="@anim/nav_default_pop_enter_anim"
        app:popExitAnim="@anim/nav_default_pop_exit_anim"
        app:popUpTo="@id/mainFragment"
        app:popUpToInclusive="true" />
    <navigation
        android:id="@+id/signUpGraph"
        app:startDestination="@id/signUpPermissionFragment">
        <argument
            android:name="emptyStateAgencyFrag"
            app:argType="boolean"
            android:defaultValue="false"/>
        <fragment
            android:id="@+id/signUpPermissionFragment"
            android:name="com.example.userapp.ui.signup.SignUpPermissionFragment"
            android:label="SignUpPermissionFragment" >
            <action
                android:id="@+id/action_signUpPermissionFragment_to_signUpAgencyFragment"
                app:destination="@id/signUpAgencyFragment"
                app:enterAnim="@anim/slide_enter"
                app:exitAnim="@anim/slide_exit"
                app:popEnterAnim="@anim/slide_pop_enter"
                app:popExitAnim="@anim/slide_pop_exit">
                <argument
                    android:name="emptyStateAgencyFrag"
                    app:argType="boolean"
                    android:defaultValue="false"/>
            </action>
        </fragment>
        <fragment
            android:id="@+id/signUpAgencyFragment"
            android:name="com.example.userapp.ui.signup.SignUpAgencyFragment"
            android:label="SignUpAgencyFragment" >
            <argument
                android:name="emptyStateAgencyFrag"
                app:argType="boolean"
                android:defaultValue="false"/>
            <action
                android:id="@+id/action_signUpAgencyFragment_to_signUpFirstFragment"
                app:destination="@id/signUpFirstFragment"
                app:enterAnim="@anim/slide_enter"
                app:exitAnim="@anim/slide_exit"
                app:popEnterAnim="@anim/slide_pop_enter"
                app:popExitAnim="@anim/slide_pop_exit"/>
        </fragment>
        <fragment
            android:id="@+id/signUpFirstFragment"
            android:name="com.example.userapp.ui.signup.SignUpFirstFragment"
            android:label="SignUpFirstFragment">
            <action
                android:id="@+id/action_signUpFirstFragment_to_signUpSecondFragment"
                app:destination="@id/signUpSecondFragment"
                app:enterAnim="@anim/slide_enter"
                app:exitAnim="@anim/slide_exit"
                app:popEnterAnim="@anim/slide_pop_enter"
                app:popExitAnim="@anim/slide_pop_exit"/>
        </fragment>
        <fragment
            android:id="@+id/signUpSecondFragment"
            android:name="com.example.userapp.ui.signup.SignUpSecondFragment"
            android:label="SignUpSecondFragment" >
            <action
                android:id="@+id/action_signUpSecondFragment_to_signUpWaitFragment"
                app:destination="@id/signUpWaitFragment"
                app:popUpTo="@id/signUpSecondFragment"
                app:popUpToInclusive="true"
                app:enterAnim="@anim/slide_enter"
                app:exitAnim="@anim/slide_exit"
                app:popEnterAnim="@anim/slide_pop_enter"
                app:popExitAnim="@anim/slide_pop_exit"/>
            <action
                android:id="@+id/action_signUpSecondFragment_pop"
                app:popUpTo="@id/signUpSecondFragment"
                app:popUpToInclusive="true"
                app:enterAnim="@anim/slide_enter"
                app:exitAnim="@anim/slide_exit"
                app:popEnterAnim="@anim/slide_pop_enter"
                app:popExitAnim="@anim/slide_pop_exit"/>
        </fragment>
        <fragment
            android:id="@+id/signUpWaitFragment"
            android:name="com.example.userapp.ui.signup.SignUpWaitFragment"
            android:label="SignUpWaitFragment" >
            <action
                android:id="@+id/action_signUpWaitFragment_pop"
                app:popUpTo="@id/signUpGraph"
                app:popUpToInclusive="true" />
        </fragment>
    </navigation>
    <navigation
        android:id="@+id/signInGraph"
        app:startDestination="@id/signInFragment">
        <fragment
            android:id="@+id/signInFragment"
            android:name="com.example.userapp.ui.signin.SignInFragment"
            android:label="SignInFragment" >
            <action
                android:id="@+id/action_signInFragment_to_mainFragment"
                app:destination="@id/mainFragment"
                app:popEnterAnim="@anim/slide_pop_enter"
                app:popExitAnim="@anim/slide_pop_exit"
                />
            <!--TODO : poping Checking 하기
                app:popUpTo="@+id/signInFragment"
                app:popUpToInclusive="true" -->
            <action
                android:id="@+id/action_signInFragment_to_signInFindInfoFragment"
                app:destination="@id/signInFindInfoFragment"
                app:enterAnim="@anim/slide_enter"
                app:exitAnim="@anim/slide_exit"
                app:popEnterAnim="@anim/slide_pop_enter"
                app:popExitAnim="@anim/slide_pop_exit" >
            </action>
        </fragment>
        <fragment
            android:id="@+id/signInFindInfoFragment"
            android:name="com.example.userapp.ui.signin.SignInFindInfoFragment"
            android:label="SignInFindInfoFragment">
            <action
                android:id="@+id/action_signInFindInfoFragment_to_signInFindInfoResultFragment"
                app:destination="@id/signInFindInfoResultFragment"
                app:popUpTo="@id/signInFindInfoFragment"
                app:popUpToInclusive="true"
                app:exitAnim="@anim/slide_exit"
                app:popEnterAnim="@anim/slide_pop_enter"
                app:popExitAnim="@anim/slide_pop_exit">
            </action>
        </fragment>
        <fragment
            android:id="@+id/signInFindInfoResultFragment"
            android:name="com.example.userapp.ui.signin.SignInFindInfoResultFragment"
            android:label="SignInFindInfoResultFragment">
            <action
                android:id="@+id/action_signInFindInfoResultFragment_pop"
                app:popUpTo="@id/signInFindInfoResultFragment"
                app:popUpToInclusive="true"
                app:popEnterAnim="@anim/slide_pop_enter"
                app:popExitAnim="@anim/slide_pop_exit" />
            <action
                android:id="@+id/action_signInFindInfoResultFragment_to_signInFindInfoFragment"
                app:destination="@id/signInFindInfoFragment"
                app:popUpTo="@id/signInFindInfoResultFragment"
                app:popUpToInclusive="true" >
            </action>
        </fragment>
    </navigation>
    <fragment
        android:id="@+id/communityPostMarketFragment"
        android:name="com.example.userapp.ui.main.community.post.CommunityPostMarketFragment"/>
    <fragment
        android:id="@+id/communityPreviewFragment"
        android:name="com.example.userapp.ui.main.community.preview.CommunityPreviewFragment">
        <action
            app:destination="@+id/communityPostFragment"
            android:id="@+id/action_communityPreview_to_communityPost"/>
        <action
            app:destination="@+id/communityWriteEmergencyFragment"
            android:id="@+id/action_communityPreview_to_communityWriteEmergency"/>
        <action
            app:destination="@+id/communityWriteFreeFragment"
            android:id="@+id/action_communityPreview_to_communityWriteFree"/>
        <action
            app:destination="@+id/communityWriteSuggestFragment"
            android:id="@+id/action_communityPreview_to_communityWriteSuggest"/>
        <action
            app:destination="@+id/communityWriteWithFragment"
            android:id="@+id/action_communityPreview_to_communityWriteWith"/>
        <action
            app:destination="@+id/communityWriteMarketFragment"
            android:id="@+id/action_communityPreview_to_communityWriteMarket"/>
        <action
            app:destination="@+id/communityPostMarketFragment"
            android:id="@+id/action_communityPreview_to_communityPostMarket"/>
        <argument
            app:argType="string"
            android:name="collection_name"
            android:defaultValue="0"/>
    </fragment>
    <fragment
        android:id="@id/communityWriteFreeFragment"
        android:name="com.example.userapp.ui.main.community.write.CommunityWriteFreeFragment">
        <action
            android:id="@+id/action_communityWriteFree_to_communityGetPhoto"
            app:destination="@+id/communityGetPhotoFragment"/>
        <action
            android:id="@+id/action_communityWriteFree_to_communityPost"
            app:destination="@+id/communityPostFragment"
            app:popUpTo="@id/communityPreviewFragment"/>
        <argument
            app:argType="string"
            android:name="modify"
            android:defaultValue="0"/>
        <argument
            app:argType="string"
            android:name="postId"
            android:defaultValue="0"/>
    </fragment>
    <fragment
        android:id="@id/communityWriteEmergencyFragment"
        android:name="com.example.userapp.ui.main.community.write.CommunityWriteEmergencyFragment">
        <action
            android:id="@+id/action_communityWriteEmergency_to_communityGetPhoto"
            app:destination="@+id/communityGetPhotoFragment"/>
        <action
            android:id="@+id/action_communityWriteEmergency_to_communityPost"
            app:destination="@+id/communityPostFragment"
            app:popUpTo="@id/communityPreviewFragment"/>
        <argument
            app:argType="string"
            android:name="modify"
            android:defaultValue="0"/>
        <argument
            app:argType="string"
            android:name="postId"
            android:defaultValue="0"/>
    </fragment>
    <fragment
        android:id="@id/communityWriteSuggestFragment"
        android:name="com.example.userapp.ui.main.community.write.CommunityWriteSuggestFragment">
        <action
            android:id="@+id/action_communityWriteSuggest_to_communityGetPhoto"
            app:destination="@+id/communityGetPhotoFragment"/>
        <action
            android:id="@+id/action_communityWriteSuggest_to_communityPost"
            app:destination="@+id/communityPostFragment"
            app:popUpTo="@id/communityPreviewFragment"/>
        <argument
            app:argType="string"
            android:name="modify"
            android:defaultValue="0"/>
        <argument
            app:argType="string"
            android:name="postId"
            android:defaultValue="0"/>
    </fragment>
    <fragment
        android:id="@id/communityWriteWithFragment"
        android:name="com.example.userapp.ui.main.community.write.CommunityWriteWithFragment">
        <action
            android:id="@+id/action_communityWriteWith_to_communityGetPhoto"
            app:destination="@+id/communityGetPhotoFragment"/>
        <action
            android:id="@+id/action_communityWriteWith_to_communityPost"
            app:destination="@+id/communityPostFragment"
            app:popUpTo="@id/communityPreviewFragment"/>
        <argument
            app:argType="string"
            android:name="modify"
            android:defaultValue="0"/>
        <argument
            app:argType="string"
            android:name="postId"
            android:defaultValue="0"/>
    </fragment>
    <fragment
        android:id="@id/communityWriteMarketFragment"
        android:name="com.example.userapp.ui.main.community.write.CommunityWriteMarketFragment">
        <action
            android:id="@+id/action_communityWriteMarket_to_communityGetPhoto"
            app:destination="@+id/communityGetPhotoFragment"/>
        <action
            android:id="@+id/action_communityWriteMarket_to_communityPostMarket"
            app:destination="@+id/communityPostMarketFragment"
            app:popUpTo="@id/communityPreviewFragment"/>
        <argument
            app:argType="string"
            android:name="modify"
            android:defaultValue="0"/>
        <argument
            app:argType="string"
            android:name="postId"
            android:defaultValue="0"/>
    </fragment>
    <fragment
        android:id="@+id/communityPostFragment"
        android:name="com.example.userapp.ui.main.community.post.CommunityPostFragment">
        <action
            android:id="@+id/action_communityPost_to_communityPreview"
            app:destination="@+id/communityPreviewFragment" />
        <action
            android:id="@+id/action_communityPost_to_communityEmergencyWrite"
            app:destination="@+id/communityWriteEmergencyFragment"/>
        <action
            android:id="@+id/action_communityPost_to_communityFreeWrite"
            app:destination="@+id/communityWriteFreeFragment"/>
        <action
            android:id="@+id/action_communityPost_to_communityWithWrite"
            app:destination="@+id/communityWriteWithFragment"/>
        <action
            android:id="@+id/action_communityPost_to_communitySuggestWrite"
            app:destination="@+id/communityWriteSuggestFragment"/>
        <action
            android:id="@+id/action_communityPost_to_communityMarketWrite"
            app:destination="@+id/communityWriteMarketFragment"/>
        <argument
            app:argType="string"
            android:name="collection_name"
            android:defaultValue="0"/>
        <argument
            app:argType="string"
            android:name="document_name"
            android:defaultValue="0"/>
    </fragment>
    <fragment
        android:id="@id/communityGetPhotoFragment"
        android:name="com.example.userapp.ui.main.community.write.CommunityGetPhotoFragment">
        <argument
            app:argType="string"
            android:name="collection_name"
            android:defaultValue="0"/>
        <action
            android:id="@+id/action_communityGetPhoto_to_communityWriteFree"
            app:destination="@id/communityWriteFreeFragment"
            app:popUpTo="@id/communityGetPhotoFragment"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_communityGetPhoto_to_communityWriteEmergency"
            app:destination="@id/communityWriteEmergencyFragment"
            app:popUpTo="@id/communityGetPhotoFragment"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_communityGetPhoto_to_communityWriteSuggest"
            app:destination="@id/communityWriteSuggestFragment"
            app:popUpTo="@id/communityGetPhotoFragment"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_communityGetPhoto_to_communityWriteWith"
            app:destination="@id/communityWriteWithFragment"
            app:popUpTo="@id/communityGetPhotoFragment"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_communityGetPhoto_to_communityWriteMarket"
            app:destination="@id/communityWriteMarketFragment"
            app:popUpTo="@id/communityGetPhotoFragment"
            app:popUpToInclusive="true"/>
        <action
            android:id="@+id/action_communityGetPhotoFragment_pop"
            app:popUpTo="@id/communityGetPhotoFragment"
            app:popUpToInclusive="true" />
    </fragment>

<!--    reservation-->
    <fragment
        android:id="@+id/reservationFacilitySelectFragment"
        android:name="com.example.userapp.ui.main.reservation.ReservationFacilitySelect">
        <argument
            android:name="myArg"
            app:argType="com.example.userapp.data.model.ReservationFacility" />
    </fragment>
    <fragment
        android:id="@+id/settingsChangeInfoFragment"
        android:name="com.example.userapp.ui.main.settings.SettingsChangeInfoFragment"
        android:label="SettingsChangeInfoFragment" >
        <action
            android:id="@+id/action_settingsChangeInfoFragment_pop"
            app:popUpTo="@id/settingsChangeInfoFragment"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpToInclusive="true" />
        <action
            android:id="@+id/action_settingsChangeInfoFragment_to_settingsChangeInfoDetailFragment"
            app:destination="@id/settingsChangeInfoDetailFragment"
            app:enterAnim="@anim/slide_enter"
            app:exitAnim="@anim/slide_exit"
            app:popEnterAnim="@anim/slide_pop_enter"
            app:popExitAnim="@anim/slide_pop_exit">
            <argument
                android:name="changeInfoType"
                app:argType="com.example.userapp.ui.main.settings.ChangeInfoType"
                app:nullable="true"
                android:defaultValue="@null" />
        </action>
    </fragment>
    <fragment
        android:id="@+id/settingsChangeInfoDetailFragment"
        android:name="com.example.userapp.ui.main.settings.SettingsChangeInfoDetailFragment"
        android:label="SettingsChangeInfoDetailFragment" >
        <argument
            android:name="changeInfoType"
            app:argType="com.example.userapp.ui.main.settings.ChangeInfoType"
            app:nullable="true"
            android:defaultValue="@null" />
        <action
            android:id="@+id/action_settingsChangeInfoDetailFragment_pop"
            app:popUpTo="@id/settingsChangeInfoDetailFragment"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpToInclusive="true" />
    </fragment>
    <fragment
        android:id="@+id/settingsChangePwdFragment"
        android:name="com.example.userapp.ui.main.settings.SettingsChangePwdFragment"
        android:label="SettingsChangePwdFragment" >
        <action
            android:id="@+id/action_settingsChangePwdFragment_pop"
            app:popUpTo="@id/settingsChangePwdFragment"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpToInclusive="true" />
    </fragment>

</navigation>