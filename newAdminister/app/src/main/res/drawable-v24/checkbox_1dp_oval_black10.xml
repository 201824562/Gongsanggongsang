<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_checked="true"
        android:drawable="@drawable/checkbox_checked_1dp_oval_orange" />
    <!-- 체크박스 선택상태 -->
    <item android:state_pressed="true"
        android:drawable="@drawable/checkbox_checked_1dp_oval_orange" />
    <!-- 체크박스 해제상태 -->
    <item android:state_pressed="false"
        android:drawable="@drawable/checkbox_unchecked_1dp_oval" />
    <!-- Default State -->
    <item android:drawable="@drawable/checkbox_unchecked_1dp_oval" />
</selector>
